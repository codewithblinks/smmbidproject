            yaxis: {
              show: false,
            },
            tooltip: {
              x: {
                show: false,
              },
              y: {
                title: {
                  formatter: (formatter = () => {
                    return "";
                  }),
                },
              },
            },

            if(isDark) {
              option["fill"] = {
                type: "gradient",
                gradient: {
                  type: "vertical",
                  shadeIntensity: 1,
                  inverseColors: !1,
                  opacityFrom: 0.3,
                  opacityTo: 0.05,
                  stops: [100, 100],
                },
              };
            },
          };
        },
      }));
    });
  </script>
