          <script>
            async function fetchWeeklyProgress() {
              try {
                const response = await fetch('/weekly-progress');
                const data = await response.json();

                if (response.ok) {
                  document.getElementById('progress-bar').style.width = data.progress + '%';
                  document.getElementById('progress-bar-count').innerText = Math.floor(data.progress) + '%';
                  document.getElementById('progress-text').innerText = ` ₦ ${data.totalSuccessfulTransaction} / ₦ 20,000.00`;

                  const number = parseFloat(data.totalSuccessfulTransaction.replace(/,/g, ''));

                  if (number  >= 20000 ) {
                    document.getElementById('win').style.display = 'block';
                    document.getElementById('show').style.display = 'none';

                  }

                } else {
                  console.error('Error fetching progress:', data.error);
                }
              } catch (error) {
                console.error('Error fetching progress:', error);
              }
            }

            // Call the function on page load
            window.onload = fetchWeeklyProgress;
          </script>

          <script>
            document.addEventListener('DOMContentLoaded', () => {
              // Select all divs with the class 'log-entry'
              const logEntries = document.querySelectorAll('.log-entry');

              // Loop through each div and add a click event listener
              logEntries.forEach(logEntry => {
                logEntry.addEventListener('click', async function () {
                  // Get the data-id attribute value
                  const activityId = this.getAttribute('data-id');

                  // Confirm with the user if they want to delete the item
                  const confirmDelete = confirm('Are you sure you want to delete this activity?');
                  if (!confirmDelete) {
                    return; // If not confirmed, don't proceed
                  }

                  try {
                    // Send DELETE request to the backend
                    const response = await fetch(`/delete/activity-log/${activityId}`, {
                      method: 'DELETE',
                      headers: {
                        'Content-Type': 'application/json'
                      }
                    });

                    if (response.ok) {
                      this.remove();
                    } else {
                      console.error('Failed to delete activity:', activityId);
                    }
                  } catch (error) {
                    console.error('Error deleting activity:', error);
                  }
                });
              });
            });
          </script>

