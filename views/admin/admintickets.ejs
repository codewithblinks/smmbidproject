            // Fetch Tickets from the Backend
            async function fetchTickets() {
              try {
                const response = await fetch("/api/admin/tickets");
                tickets = await response.json();
                activeTLL = tickets.length || 0;
                  updateTicketCounts();
                  renderTickets(tickets);
              } catch (error) {
                console.error("Error fetching active tickets:", error);
              }
            }

            async function fetchSolvedTickets() {
              try {
                const response = await fetch("/api/admin/solved/tickets");
                solvedTickets = await response.json();
                solvedTLL = solvedTickets.length || 0;
                updateTicketCounts();
                renderSolvedTickets(solvedTickets);
              } catch (error) {
                console.error("Error fetching solved tickets:", error);
              }
