              fetchSolvedTickets();
              toggleView(false);
              listpageBTN.style.display = "flex";
              backheaderBTN.style.display = "none";
              typeMSG.style.display = "none"
            });

            document.getElementById('activeTicketBTN').addEventListener('click', () => {
              fetchTickets();
              toggleView(false);
              listpageBTN.style.display = "flex";
              backheaderBTN.style.display = "none";
              typeMSG.style.display = "none"
            });

            fetchTickets();

          </script>


