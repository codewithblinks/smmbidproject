                <div class="space-y-2">
                <div class="flex items-center gap-3">
                <div class="rounded-md bg-black/10 p-4 py-2 dark:bg-gray-800 
                ${message.sender === 'Admin' ? 'ltr:rounded-br-none rtl:rounded-bl-none !bg-primary text-white'
                 : 'ltr:rounded-bl-none rtl:rounded-br-none'}" 
                >${message.message}
                </div>       
                  </div>
                  <div class="text-xs text-white-dark ${
                    isUserMessage ? "ltr:text-right rtl:text-left" : ""
                      }">${message.timeSince}</div>
                    </div>
                </div>
                  `;
                }

                
