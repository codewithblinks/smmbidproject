            async function postread(ticketId) {
              try {
                const response = await fetch(`/api/tickets/admin/${ticketId}/read`, {
                    method: "POST",
                    headers: {
                      "Content-Type": "application/json",
                    },
                    body: JSON.stringify({ ticketId }),
                  });


                if (!response.ok) {
                  console.error(`API error: ${response.status} ${response.statusText}`);
                  return;
                }

                const read = await response.json();

                if (!read || read.length === 0) {
                  console.log("No updates available for this ticket.");

                } else {
                  console.log("Ticket marked as read successfully:", read);

                }
              } catch (error) {
                console.error("Error fetching messages:", error);
              }
            }


            // Fetch Messages for a Specific Ticket
            async function fetchMessages(ticketId) {
              try {
                const response = await fetch(`/api/admin/tickets/${ticketId}/messages`);

                if (!response.ok) {
                console.error(`API error: ${response.status} ${response.statusText}`);
                return;
                 }

                messages = await response.json();
                console.log(messages)
                toggleView(true);

                if (!messages || messages.length === 0) {
                    renderNoMessagesPlaceholder(ticketId);
                    renderMessageButton(ticketId)
                  } else {
                    renderMessages();
                    renderMessageButton(ticketId)
                  }
              } catch (error) {
                console.error("Error fetching messages:", error);
              }
            }

            function renderMessages() {
              const chatContainer = document.getElementById("chatContainer");
              chatContainer.innerHTML = messages
              .map((message) => createMessageElement(message))
              .join('');
              // Scroll to the bottom of the chat
              chatContainer.scrollTop = chatContainer.scrollHeight;
            }

            function createMessageElement(message) {
            const isUserMessage = message.user_id === loggedInUserId;

