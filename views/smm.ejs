          <script>
            async function buyService() {
              const buyButton = document.getElementById("buyButton");
              buyButton.disabled = true;
              buyButton.innerText = "Processing...";

              try {
                const response = await fetch('/buysmm', {
                  method: 'POST',
                  headers: {
                    'Content-Type': 'application/json',
                  },
                  body: JSON.stringify({
                    serviceId: document.getElementById('servicesMore').value,
                    link: document.getElementById('link').value,
                    quantity: document.getElementById('quantity').value,
                    service: document.getElementById('servicesMore1').value,
                    amount: document.getElementById('amount').value
                  })
                });

                const result = await response.json();

                if (response.ok) {
                  toastr.success("Service purchase successful, processing now.");
                  window.location.href = "/orderhistory";
                } else {
                  toastr.error(result.error || "An error occurred. Please try again.");
                  buyButton.disabled = false;
                  buyButton.innerText = "Buy";
                }
              } catch (error) {
                console.error("Request failed:", error);
                toastr.error("Failed to complete purchase. Please try again.");
                buyButton.disabled = false;
                buyButton.innerText = "Buy";
              }
            }
          </script>

